{
    "name": "sphil",
    "version": "0.1.0",
    "private": true,
    "scripts": {
        "h": "jq '.scripts' package.json",
        "dev": "next dev --turbo",
        "email:dev": "email dev --dir lib/email/templates",
        "build": "next build",
        "start": "next start",
        "lint": "next lint",
        "ct": "tsc --pretty --noEmit",
        "cd:dev": "gh workflow run cd_dev.yaml --ref $(git rev-parse --abbrev-ref HEAD)",
        "prettier:check": "npx prettier --check .",
        "postbuild": "pagefind --site .next/standalone/.next/server/app/articles --output-path public/_pagefind",
        "stripe:listen": "stripe listen --forward-to 127.0.0.1:3000/api/stripe-webhook",
        "db:studio": "dotenv -e .env -- npx prisma studio",
        "db:studio:prod": "dotenv -e .env.production -- npx prisma studio",
        "db:regen": "npx prisma generate",
        "db:push": "npx prisma db push",
        "db:push:prod": "dotenv -e .env.production -- npx prisma db push",
        "email:send-news": "dotenv -e .env -- bun run scripts/email/send-newsletter.tsx",
        "email:send-news:prod": "dotenv -e .env.production -- bun run scripts/email/send-newsletter.tsx",
        "email:send-seminars": "bun run scripts/email/send-seminar-email.tsx",
        "email:send-reminders": "bun run scripts/email/send-seminar-reminder.tsx"
    },
    "dependencies": {
        "@auth/prisma-adapter": "^2.8.0",
        "@dnd-kit/core": "^6.3.1",
        "@dnd-kit/sortable": "^10.0.0",
        "@emotion/react": "^11.13.0",
        "@emotion/styled": "^11.13.0",
        "@google-cloud/storage": "^7.13.0",
        "@mdxeditor/editor": "3.14.0",
        "@mui/icons-material": "^6.4.0",
        "@mui/material": "^6.4.0",
        "@mui/x-date-pickers": "^7.28.3",
        "@prisma/client": "^6.4.1",
        "@react-email/components": "^0.0.36",
        "@tailwindcss/postcss": "^4.0.1",
        "@tailwindcss/typography": "^0.5.16",
        "@types/node": "20.6.0",
        "@types/react": "18.3.1",
        "@types/react-dom": "18.3.1",
        "classnames": "^2.5.1",
        "clsx": "^2.1.1",
        "dayjs": "^1.11.13",
        "eslint": "8.49.0",
        "eslint-config-next": "13.4.19",
        "framer-motion": "^10.18.0",
        "next": "15.4.6",
        "next-auth": "^5.0.0-beta.25",
        "nextra": "4.3.0",
        "nextra-theme-docs": "4.3.0",
        "posthog-js": "^1.230.1",
        "prettier": "3.4.2",
        "prisma": "^6.4.1",
        "react": "18.3.1",
        "react-cookie-consent": "^9.0.0",
        "react-dom": "18.3.1",
        "react-email": "^4.0.6",
        "react-hook-form": "^7.53.1",
        "react-hot-toast": "^2.4.1",
        "react-textarea-autosize": "^8.5.4",
        "rehype-external-links": "^3.0.0",
        "remark-directive": "^3.0.0",
        "remark-gfm": "^4.0.0",
        "resend": "^4.1.1",
        "stripe": "^17.2.1",
        "tailwind-merge": "^2.6.0",
        "tailwindcss": "^4.0.1",
        "typescript": "5.7.3"
    },
    "optionalDependencies": {
        "@napi-rs/simple-git-linux-x64-gnu": "^0.1.9",
        "@napi-rs/simple-git-linux-x64-musl": "^0.1.16"
    },
    "devDependencies": {
        "@svgr/webpack": "^8.0.1",
        "daisyui": "^5.0.0-beta.1",
        "dotenv-cli": "^8.0.0",
        "pagefind": "1.3.0"
    }
}
